
.list {
  width: 100%;
  height: 80rpx;
  border-bottom: 2rpx solid #eeeeee;
  border-top: 2rpx solid #eeeeee;
  display: flex;
  font-size: 30rpx;
}
.list > .title {
  width: 150rpx;
  height: 100%;
  border-right: 2rpx solid #ccc;
  text-align: center;
  line-height: 80rpx;
}
.lists {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #ccc;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list > .inp {
  flex: 1;
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}